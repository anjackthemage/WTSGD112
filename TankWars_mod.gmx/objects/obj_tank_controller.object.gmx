          <argument>
            <kind>1</kind>
            <string>/// Move the appropriate tank based upon key being pressed



// Cycle through controls for tank one
var cur_tank = obj_tank_1;

var cur_control = ds_map_find_first(tank_one_control);

do
{
    if (string_length(tank_one_control[? cur_control]) &gt; 1)
    {
        // This is a special key (non-alphanumeric)
        if(keyboard_check(tank_one_control[? cur_control]))
        {
            move_tank(cur_tank, cur_control);
        }
    }
    else
    {
        // This is a regular alphanumeric key
        if(keyboard_check(ord(tank_one_control[? cur_control])))
        {
            move_tank(cur_tank, cur_control);
        }
    }
    cur_control = ds_map_find_next(tank_one_control, cur_control);
}
until(is_undefined(cur_control))
</string>
