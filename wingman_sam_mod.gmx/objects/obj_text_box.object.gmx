        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// If mouse clicked on me, I have focus

was_clicked = point_in_rectangle(mouse_x, mouse_y, x - 100, y - 10, x + 100, y + 10);

if(!has_focus and was_clicked)
{
    has_focus = true;
}
else if (has_focus and !was_clicked)
{
    has_focus = false;
}
</string>
