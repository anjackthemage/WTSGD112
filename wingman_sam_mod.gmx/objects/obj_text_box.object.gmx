        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Get text

// de-bounce
if(keyboard_delay &lt;= 0)
{
    if(keyboard_check(vk_backspace))
    {
        if(string_length(text_input_string) &gt; 1)
        {
            text_input_string = string_delete(text_input_string, string_length(text_input_string) - 1, 1);
        }
        else
        {
            text_input_string = "";
        }
        keyboard_delay--;
        keyboard_lastchar = '';
    }
    else if (keyboard_lastchar != '')
    {
        text_input_string += keyboard_lastchar;
        keyboard_delay--;
        keyboard_lastchar = '';
    }
}
else
{
    keyboard_delay--;
}
</string>
          </argument>
        </arguments>
      </action>
