        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Draw me like one of your French boxes

draw_set_font(fnt_panel);
draw_set_valign(fa_middle);
// Total width is text string, plus padding, plus box width
total_entity_width = string_width(box_label) + 210;

// Set label origin
label_x = x - (total_entity_width / 2);
label_y = y;
// Set box origin and dimensions
box_x = label_x + string_width(box_label);
box_y = y - 10;
box_width = 200;
box_height = 20;

// Draw label
draw_set_color(c_white);
draw_text(label_x, label_y, box_label);

// Draw box background (semi-transparent)
draw_set_color(c_navy);
draw_set_alpha(0.5);
draw_rectangle(box_x, box_y, box_x + box_width, box_y + box_height, false);
// Make the box border semi-transparent if not in focus
if(!has_focus)
{
    draw_set_alpha(0.5);
    cur_cursor = "";
}
else
{
    draw_set_alpha(1);
    cur_cursor = text_cursor;
}
// Draw the box border
draw_set_color(c_white);
draw_rectangle(box_x, box_y, box_x + box_width, box_y + box_height, true);
// Draw text input
draw_set_halign(fa_center);
draw_text(box_x + 100, y, text_input_string + cur_cursor);
// Ensure alpha is reset
draw_set_alpha(1);
</string>
          </argument>
        </arguments>
      </action>
